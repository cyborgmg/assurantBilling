/* barra --> configuracao comum a todas as barras */
.barras { display: block; width:95%; margin:10px auto; padding:8px 20px; border:1px solid #ddd; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; }

/* menu --> topo */
ul                      { list-style: none; cursor: default}
.barrasMenu             { display: block; width:97%; margin:10px auto; padding:8px 9px; border:1px solid #ddd; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; }
.menu                   { border-bottom-left-radius:8px; border-bottom-right-radius:8px; padding: 11px 6px}
.menu .menuSetup        { width: 150px;  float: left; padding: 8px 0px; border-collapse: collapse; text-align:center; color:#09445d; font-size: 14px; font-weight:bold;  border-left: 1px solid #ddd; border-top: 1px solid #fff; border-bottom: 1px solid #fff; border-right:1px solid #ddd;}
.menu .menuSetup        { margin: -16px 0px 0px 0px}
.menu .menuSetup:hover  { padding: 8px 0px; border:1px solid #fd7802; text-decoration: none; color:#fff; background: #fff url(../img/menu/corMenu.png)repeat-x;}
.menuSetup a            { color:#555; }
.menuSetup a:hover      { color: #0B55C4; text-decoration: none}

.menuReq ul            { width: 200px; position: absolute; margin-top: 9px; left: 137px; z-index: 1}
.menuReq ul li         { padding: 8px 25px; background: #fff; text-align:left; color:#09445d; font-weight:bold; border-left: 1px solid #eee; border-top: 1px solid #eee; border-right: 1px solid #ddd; border-bottom: 1px solid #ddd;}
.menuReq ul li:hover   { padding: 8px 25px; border:1px solid #fd7802; text-decoration: none; color:#fff; background: #fff url(../img/menu/corMenu.png) repeat-x;}

.menuConf ul            { width: 230px; position: absolute; margin-top: 9px; left: 441px; z-index: 1}
.menuConf ul li         { padding: 8px 25px; background: #fff; text-align:left; color:#09445d; font-weight:bold; border-left: 1px solid #eee; border-top: 1px solid #eee; border-right: 1px solid #ddd; border-bottom: 1px solid #ddd;}
.menuConf ul li:hover   { padding: 8px 25px; border:1px solid #fd7802; text-decoration: none; color:#fff; background: #fff url(../img/menu/corMenu.png) repeat-x;}

#Marcado{
   border-right:  1px solid #6FADE6;
   background:    #fff url(../img/menu/corMenu.png)repeat-x;
   color:         #000;
}

#downArrow{ margin-left: 14px; margin-bottom: 2px; }

/* barra --> configuracao especifica de cada barra */
.barras.sul p { text-align:center; margin:0; padding:8px 0; }


/* barra --> topo */
.barras .topo { width: 100%; }
.barras .topo td { width: 50%; font-size: 18px; font-weight: bold;  }
.barras .topo td img { vertical-align: middle; }
.barras .topo td.botoes { text-align: right; }
.barras .topo td.versao { text-align: right; font-size: 10px; }
.barras .topo a { display: inline-block; position: relative; width: 24px; margin: 0; padding: 4px; background: #eee; border: 1px solid #ddd; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; }
.barras .topo a:hover { background: #fff; border: 1px solid #ddd; }

.identificaoUsuario { color: #f57021; }

/* barra --> navegacao */
.barras .navegacao { width:960px; margin: 0 auto;}
.barras .navegacao td {  }
.barras .navegacao td.botoesRetornar{ width:38%; text-align: right; }
.barras .navegacao td.botoesCentro{ width:23%; text-align: center; font-size: 28px; font-weight: bold; color: #ccc; }
.barras .navegacao td.botoesAvancar{ width:38%; text-align: left; }
.barras .navegacao a { display: inline-block; position: relative; width: 58px; margin:0 4px; padding: 4px 6px; background: #eee; border: 1px solid #ccc; color: #384884; text-decoration: none; font-size: 10px; font-weight: normal; text-align: center; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; }
.barras .navegacao a:hover { border: 1px solid #eee; background:#fff; }


/* tabela --> formul�rios */
.tabelas { padding: 10px 20px; }
.tabelas table { width:100%; border-collapse: collapse; }
.tabelas table td { padding: 4px 10px 4px 2px; text-align: left; }
.tabelas table th { border: 1px solid #eee; padding: 4px 10px; text-align: left; }
.tabelas .label { width: 20%; text-align: right; }
.tabelas .tabelaReadWrite2 .label { width: 16%; text-align: right; }
.tabelas table td.label{ border: 1px solid white; }
.h2Esquerda { text-align: left; }

.multiSelect { min-width: 190px; border: 1px solid #ddd; max-width: 200px; max-height: 180px;}
table.TabelaPrin { width: 100%; border-spacing: 1px; color: #666;}
table.TabelaPrin td,
table.TabelaPrin th { padding: 1px;}
table.TabelaPrin thead th { text-align: center; background: #f0f0f0; color: #0B55C4; font-size:14px; border-bottom: 0px solid #999; border-left: 0px solid #fff;}
table.TabelaPrin thead a:hover { text-decoration: none;}
table.TabelaPrin tbody th { font-weight: bold;}
table.TabelaPrin tbody tr { background-color: #fff;  text-align: left;}
table.TabelaPrin tbody tr.even 	{ background: #f9f9f9; border-top: 1px solid #fff;}
table.TabelaPrin tbody tr.odd:hover td, table.TabelaPrin tbody tr.even:hover td  { background-color: #e6f0ff ;}
table.TabelaPrin tbody tr td { background: #fff; border: 1px solid #fff; line-height: 20px; padding: 0px 10px 0px 2px;}
table.TabelaPrin tbody tr td.Label { background: #c5cadd; border: 1px solid #ccc; height: 32px;}
table.TabelaPrin tbody tr td.upload { vertical-align: top; padding-top: 10px;}
table.TabelaPrin tbody tr td.Label label { font-family: Arial, Helvetica, sans-serif; font-size: 12px; color: #243a4f;}
table.TabelaPrin tbody tr.even td { background: #f9f9f9; border-top: 1px solid #FFF;line-height: 20px;}
table.TabelaPrin tbody tr td input { border: 1px solid #CDD3D8; padding: 4px 8px; background: #eee;}
table.multiSelectTable tbody td table tbody tr th { padding-left: 4px; font-family: Arial, Helvetica, sans-serif; font-size: 12px; color: #243a4f; border: none;}
table.TabelaPrin td.order { text-align: center; white-space: nowrap; }
table.TabelaPrin td.order span { float: left; display: block; width: 15px; text-align: center; }
.Paginacao { padding-left:240px; padding-top:5px; padding-bottom:5px; margin:0 auto; background-color: #f3f3f3; border-top: 1px solid #999; border-left: 1px solid #ddd; border-right: 1px solid #ddd; border-bottom: 1px solid #ddd; display:block; text-align:center; height:22px;}


/* tabela --> login */
.BoxModal { display:block; width:90%; padding:0; margin: 20px auto; }
.BoxModal label { padding:4px 0px; }
.BoxModal .IconeInputCpf { font-weight: bold; color:#999; padding-top:6px;padding-bottom:6px; width:200px; }
.LoginBtn { width: 100%; text-align: right; }


/* tabela --> display-tag NOVA */
.DisplayTag { padding: 0; text-align: center;}
.DisplayTag table { width:100%; border-collapse: collapse; }
.DisplayTag table td, .DisplayTag table th { border: 1px solid #eee; padding: 4px 10px; text-align: center; }
.DisplayTag table th { padding: 8px 10px; }
.DisplayTag table th a { text-decoration: none; }
.DisplayTag table td.label{ width: 20%; text-align: right; border: 1px solid #eee; }


#boxConfirmacao { width:70%; margin: 20px auto; padding: 40px 26px 40px 26px; background: #f5f5f5 15px 40px no-repeat; border: 1px solid #e3e3e3; color: #09445D;}

/* tabela --> para as display-tags que tiverem muita informa��o */
.AjusteDisplay table td, .AjusteDisplay table th { padding: 4px 4px; }
.AjusteDisplay table th { padding: 8px 4px; }

.paginacao {
    width:100%; 
    padding:10px 0; 
    margin:0 auto; 
    text-align:center;
}

.inicio.off, .anterior.off, .proximo.off, .fim.off, .PaginacaoNumeros.off { 
    border: 1px solid #aaaaaa; 
    background: #cccccc url(../css/images/ui-bg_highlight-soft_75_cccccc_1x100.png) 50% 50% repeat-x; 
    color: #222222; 
    font-weight: bold;
}

th.sorted a, th.sortable a {
    background-position: right;
    display: block;
    width: 100%;
}

th.sortable a {
    background-image: url(../img/icones/displaytag/seta_disable_white.png);
    background-repeat:no-repeat;
    background-position:50% right;
}

th.order1 a {
    background-image: url(../img/icones/displaytag/seta_baixo_white.png);
    background-repeat:no-repeat;
    background-position:50% right;
}

th.order2 a {
    background-image: url(../img/icones/displaytag/seta_cima_white.png);
    background-repeat:no-repeat;
    background-position:50% right;
}

