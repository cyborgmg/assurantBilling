package br.com.delphos.excecoes;

import br.com.delphos.sca.constantes.CodigoMensagem;

public class RestricaoPersistenciaException extends PersistenciaException {
	private static final long serialVersionUID = 1L;

	public RestricaoPersistenciaException() {
		super();
	}

	public RestricaoPersistenciaException(CodigoMensagem codigoRetorno, Object... args) {
		super(codigoRetorno, args);
	}

	public RestricaoPersistenciaException(String m, Throwable t) {
		super(m, t);
	}

	public RestricaoPersistenciaException(String m) {
		super(m);
	}

	public RestricaoPersistenciaException(Throwable t) {
		super(t);
	}

}
