package br.com.delphos.excecoes;

import br.com.delphos.sca.constantes.CodigoMensagem;

public class SessaoInvalidaException extends ValidacaoException {
	private static final long serialVersionUID = 1L;

	public SessaoInvalidaException() {
		super();
	}

	public SessaoInvalidaException(CodigoMensagem codigoRetorno, Object... args) {
		super(codigoRetorno, args);
	}

	public SessaoInvalidaException(String m, Throwable t) {
		super(m, t);
	}

	public SessaoInvalidaException(String m) {
		super(m);
	}

	public SessaoInvalidaException(Throwable t) {
		super(t);
	}

}
